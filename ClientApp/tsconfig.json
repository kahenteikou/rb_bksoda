{
    "compilerOptions": {
      "target": "ES2022",
      "module": "esnext",
      "lib": ["esnext", "dom"],
      "moduleResolution": "node",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
      "outDir": "./build",
      "sourceRoot": "./src",
    }
}